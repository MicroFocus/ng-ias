{
  "compilerOptions": {
    "baseUrl": "./",
    "experimentalDecorators": true,
    "module": "commonjs",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "outDir": "target/js"
  },
  "compileOnSave": true,
  "files": [
  ],
  "exclude": [
    "dist",
    "node_modules"
  ]
}
